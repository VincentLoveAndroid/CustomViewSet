<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_zuojia_enter"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.mingren.customviewset.activity.ZuojiaEnterActivity">


    <TextView
        android:id="@+id/tv_test"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_centerHorizontal="true"
        android:layout_marginRight="50dp"
        android:layout_marginTop="50dp"
        android:text="座驾进场"
        android:textSize="25sp" />


    <TextView
        android:id="@+id/tv_end"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_alignParentLeft="true"
        android:layout_marginTop="50dp"
        android:text="end"
        android:textSize="25sp" />

    <com.example.mingren.customviewset.view.zuojia.ZuojiaEnterView
        android:id="@+id/zuojiaView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"></com.example.mingren.customviewset.view.zuojia.ZuojiaEnterView>
</RelativeLayout>
